      <dataType>TEXT</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="ventricularRate">
      <name>ventricularRate</name>
      <description>Ventricular_Rate</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>BPM</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="pQInterval">
      <name>PQInterval</name>
      <description>PQ_Interval</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="pDuration">
      <name>PDuration</name>
      <description>P_Duration</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="qRsDuration">
      <name>QRsDuration</name>
      <description>QRS_Duration</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="qTInterval">
      <name>QTInterval</name>
      <description>QT_Interval</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="qTCInterval">
      <name>QTCInterval</name>
      <description>QTC_Interval</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="rRInterval">
      <name>RRInterval</name>
      <description>RR_Interval</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="pPInterval">
      <name>PPInterval</name>
      <description>PP_Interval</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="pAxis">
      <name>PAxis</name>
      <description>P_Axis</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>degree</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="rAxis">
      <name>RAxis</name>
      <description>R_Axis</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>degree</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="tAxis">
      <name>TAxis</name>
      <description>T_Axis</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>degree</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="qRSNum">
      <name>QRSNum</name>
      <description>QRS_Number</description>
      <dataType>INTEGER</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="pOnset">
      <name>POnset</name>
      <description>P_Onset</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="pOffset">
      <name>POffset</name>
      <description>P_Offset</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="qOnset">
      <name>QOnset</name>
      <description>Q_Onset</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="qOffset">
      <name>QOffset</name>
      <description>Q_Offset</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="tOffset">
      <name>TOffset</name>
      <description>T_Offset</description>
      <dataType>INTEGER</dataType>
      <measurementUnit>ms</measurementUnit>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="xmlFile">
      <name>xmlFile</name>
      <description>Xml_File</description>
      <dataType>DATA</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="pdfFile">
      <name>pdfFile</name>
      <description>Pdf_File</description>
