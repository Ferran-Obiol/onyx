<?xml version="1.0" encoding="ISO-8859-1"?>
<instrumentType>

  <name>Impedence310</name>

  <instrument>
    <name>Tanita TBF-310</name>
    <vendor>Tanita</vendor>
    <model>TBF-310</model>
    <status>ACTIVE</status>
    <serialNumber>1234567890</serialNumber>
    <barcode>CAG1001</barcode>
  </instrument>

  <contraindications>
    <contraIndication>
      <code>UnableToStandUnassistedHandicap</code>
      <type>OBSERVED</type>
    </contraIndication>

    <contraIndication>
      <code>UnableToStandUnassistedWheelchair</code>
      <type>OBSERVED</type>
    </contraIndication>

    <contraIndication>
      <code>UnableToStandUnassistedCast</code>
      <type>OBSERVED</type>
    </contraIndication>

    <contraIndication>
      <code>Other_CI</code>
      <requiresDescription>true</requiresDescription>
      <type>OBSERVED</type>
    </contraIndication>

    <contraIndication>
      <code>IrremovableMetallicDevice</code>
      <type>ASKED</type>
    </contraIndication>

  </contraindications>

  <instrumentParameters>
    <interpretative>
      <code>PhysicalActivityLast24Hours</code>
      <dataType>TEXT</dataType>
      <captureMethod>MANUAL</captureMethod>
      <type>OBSERVED</type>
    </interpretative>

    <input id="clothes_weight">
      <code>ClothesWeight</code>
      <dataType>DECIMAL</dataType>
      <measurementUnit>kg</measurementUnit>
      <captureMethod>MANUAL</captureMethod>
      <inputSource class="fixedSource">
        <value>1</value>
      </inputSource>
    </input>

    <input id="gender_in">
      <code>Gender</code>
      <dataType>TEXT</dataType>
      <captureMethod>MANUAL</captureMethod>
      <inputSource class="participantPropertySource">
        <property>gender</property>
      </inputSource>
    </input>

    <input id="body_type_in">
      <code>BodyType</code>
      <dataType>TEXT</dataType>
      <captureMethod>MANUAL</captureMethod>
      <inputSource class="fixedSource">
        <value>STANDARD</value>
      </inputSource>
    </input>

    <input id="age_in">
      <code>Age</code>
      <dataType>INTEGER</dataType>
      <captureMethod>MANUAL</captureMethod>
      <inputSource class="participantPropertySource">
        <property>age</property>
      </inputSource>
    </input>

    <input id="height_in">
      <code>Height</code>
      <measurementUnit>cm</measurementUnit>
      <dataType>INTEGER</dataType>
      <captureMethod>MANUAL</captureMethod>
      <inputSource class="outputParameterSource">
        <instrumentType>StandingHeight</instrumentType>
        <parameterName>Average_Height_Measurement</parameterName>
      </inputSource>
    </input>

    <output id="body_type">
      <vendorName>BodyType</vendorName>
      <code>Body_type</code>
      <dataType>TEXT</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <integrityChecks>
        <equalsParameterCheck>
          <targetParameter reference="body_type"/>
          <parameter reference="body_type_in"/>
          <customizedDescription>EqualsParameterCheck_BodyType</customizedDescription>
        </equalsParameterCheck>
      </integrityChecks>
    </output>

    <output id="gender">
      <vendorName>Gender</vendorName>
      <code>Output_gender</code>
      <dataType>TEXT</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <integrityChecks>
        <equalsParameterCheck>
          <targetParameter reference="gender"/>
          <parameter reference="gender_in"/>
          <customizedDescription>EqualsParameterCheck_Gender</customizedDescription>
        </equalsParameterCheck>
      </integrityChecks>
    </output>

    <output id="age">
      <vendorName>Age</vendorName>
      <code>Output_age</code>
      <dataType>INTEGER</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <integrityChecks>
        <equalsParameterCheck>
          <targetParameter reference="age"/>
          <parameter reference="age_in"/>
          <customizedDescription>EqualsParameterCheck_Age</customizedDescription>
        </equalsParameterCheck>
      </integrityChecks>
    </output>

    <output id="height">
      <vendorName>Height</vendorName>
      <code>Output_height</code>
      <measurementUnit>cm</measurementUnit>
      <dataType>INTEGER</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <integrityChecks>
        <equalsParameterCheck>
          <targetParameter reference="height"/>
          <parameter reference="height_in"/>
          <customizedDescription>EqualsParameterCheck_Height</customizedDescription>
        </equalsParameterCheck>
      </integrityChecks>
    </output>

    <output id="weight">
      <vendorName>Weight</vendorName>
      <code>Measured_weight</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>DECIMAL</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="bmi">
      <vendorName>BMI</vendorName>
      <code>Body_mass_index_calculated</code>
      <measurementUnit>kg/m²</measurementUnit>
      <dataType>DECIMAL</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
    </output>

    <output id="bmr">
      <vendorName>BMR</vendorName>
      <code>Basal_metabolic_rate_calculated</code>
      <dataType>INTEGER</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <measurementUnit>kJ</measurementUnit>
    </output>

    <output id="impedance">
      <vendorName>Impedance</vendorName>
      <code>Measured_body_impedance</code>
      <dataType>INTEGER</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <measurementUnit>ohm</measurementUnit>
    </output>

    <output id="fat_percentage">
      <vendorName>FatPercentage</vendorName>
      <code>Fat_percentage_calculated</code>
      <dataType>DECIMAL</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <measurementUnit>%</measurementUnit>
    </output>

    <output id="fat_mass">
      <vendorName>FatMass</vendorName>
      <code>Fat_mass_calculated</code>
      <dataType>DECIMAL</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <measurementUnit>kg</measurementUnit>
    </output>

    <output id="ffm">
      <vendorName>FatFreeMass</vendorName>
      <code>Fat_free_mass_calculated</code>
      <dataType>DECIMAL</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <measurementUnit>kg</measurementUnit>
    </output>

    <output id="tbw">
      <vendorName>TotalBodyWater</vendorName>
      <code>Total_Body_Water_calculated</code>
      <dataType>DECIMAL</dataType>
      <captureMethod>AUTOMATIC</captureMethod>
      <measurementUnit>kg</measurementUnit>
    </output>
  </instrumentParameters>
</instrumentType>
