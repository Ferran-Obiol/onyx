<?xml version="1.0" encoding="ISO-8859-1"?>
<instrumentType>

  <name>GripStrength</name>

  <instrument>
    <name>Dynamometer 1</name>
    <vendor>Baseline</vendor>
    <model>Hydraulic Hand Dynamometer</model>
    <barcode>mus01</barcode>
    <status>ACTIVE</status>
  </instrument>
  <contraindications>
    <contraIndication>
      <code>UnableToStandUnassistedHandicap</code>
      <type>OBSERVED</type>
    </contraIndication>

    <contraIndication>
      <code>UnableToStandUnassistedWheelchair</code>
      <type>OBSERVED</type>
    </contraIndication>

    <contraIndication>
      <code>UnableToStandUnassistedCast</code>
      <type>OBSERVED</type>
    </contraIndication>

    <contraIndication>
      <code>Other_CI</code>
      <requiresDescription>true</requiresDescription>
      <type>OBSERVED</type>
    </contraIndication>

    <contraIndication>
      <code>HandOrWristArthritisAndOtherProblem</code>
      <type>ASKED</type>
    </contraIndication>

    <contraIndication>
      <code>HandOrWristSurgery</code>
      <type>ASKED</type>
    </contraIndication>
  </contraindications>

  <instrumentParameters>
    <output id="mus1_right_strength1">
      <code>First_Right_Hand_Muscle_Strength_Measurement</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>INTEGER</dataType>
      <captureMethod>MANUAL</captureMethod>
      <integrityChecks>
        <rangeCheck>
          <targetParameter reference="mus1_right_strength1"/>
          <integerMinValueMale>0</integerMinValueMale>
          <integerMaxValueMale>90</integerMaxValueMale>
          <integerMinValueFemale>0</integerMinValueFemale>
          <integerMaxValueFemale>90</integerMaxValueFemale>
        </rangeCheck>
        <rangeCheck>
          <type>WARNING</type>
          <targetParameter reference="mus1_right_strength1"/>
          <integerMinValueMale>0</integerMinValueMale>
          <integerMaxValueMale>72</integerMaxValueMale>
          <integerMinValueFemale>0</integerMinValueFemale>
          <integerMaxValueFemale>72</integerMaxValueFemale>
        </rangeCheck>
      </integrityChecks>
    </output>

    <output id="mus1_left_strength1">
      <code>First_Left_Hand_Muscle_Strength_Measurement</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>INTEGER</dataType>
      <captureMethod>MANUAL</captureMethod>
      <integrityChecks>
        <rangeCheck>
          <targetParameter reference="mus1_left_strength1"/>
          <integerMinValueMale>0</integerMinValueMale>
          <integerMaxValueMale>90</integerMaxValueMale>
          <integerMinValueFemale>0</integerMinValueFemale>
          <integerMaxValueFemale>90</integerMaxValueFemale>
        </rangeCheck>
        <rangeCheck>
          <type>WARNING</type>
          <targetParameter reference="mus1_left_strength1"/>
          <integerMinValueMale>0</integerMinValueMale>
          <integerMaxValueMale>72</integerMaxValueMale>
          <integerMinValueFemale>0</integerMinValueFemale>
          <integerMaxValueFemale>72</integerMaxValueFemale>
        </rangeCheck>
      </integrityChecks>
    </output>

    <output id="mus1_right_strength2">
      <code>Second_Right_Hand_Muscle_Strength_Measurement</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>INTEGER</dataType>
      <captureMethod>MANUAL</captureMethod>
      <integrityChecks>
        <rangeCheck>
          <targetParameter reference="mus1_right_strength2"/>
          <integerMinValueMale>0</integerMinValueMale>
          <integerMaxValueMale>90</integerMaxValueMale>
          <integerMinValueFemale>0</integerMinValueFemale>
          <integerMaxValueFemale>90</integerMaxValueFemale>
        </rangeCheck>
        <rangeCheck>
          <type>WARNING</type>
          <targetParameter reference="mus1_right_strength2"/>
          <integerMinValueMale>0</integerMinValueMale>
          <integerMaxValueMale>72</integerMaxValueMale>
          <integerMinValueFemale>0</integerMinValueFemale>
          <integerMaxValueFemale>72</integerMaxValueFemale>
        </rangeCheck>
        <parameterSpreadCheck>
          <targetParameter reference="mus1_right_strength2"/>
          <parameter reference="mus1_right_strength1"/>
          <percent>10</percent>
        </parameterSpreadCheck>
      </integrityChecks>
    </output>

    <output id="mus1_left_strength2">
      <code>Second_Left_Hand_Muscle_Strength_Measurement</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>INTEGER</dataType>
      <captureMethod>MANUAL</captureMethod>
      <integrityChecks>
        <rangeCheck>
          <targetParameter reference="mus1_left_strength2"/>
          <integerMinValueMale>0</integerMinValueMale>
          <integerMaxValueMale>90</integerMaxValueMale>
          <integerMinValueFemale>0</integerMinValueFemale>
          <integerMaxValueFemale>90</integerMaxValueFemale>
        </rangeCheck>
        <rangeCheck>
          <type>WARNING</type>
          <targetParameter reference="mus1_left_strength2"/>
          <integerMinValueMale>0</integerMinValueMale>
          <integerMaxValueMale>72</integerMaxValueMale>
          <integerMinValueFemale>0</integerMinValueFemale>
          <integerMaxValueFemale>72</integerMaxValueFemale>
        </rangeCheck>
        <parameterSpreadCheck>
          <targetParameter reference="mus1_left_strength2"/>
          <parameter reference="mus1_left_strength1"/>
          <percent>10</percent>
        </parameterSpreadCheck>
      </integrityChecks>
    </output>

    <computedOutput>
      <code>Average_Right_Hand_Muscle_Strength_Measurement</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>INTEGER</dataType>
      <algorithm>AVERAGE</algorithm>
      <captureMethod>COMPUTED</captureMethod>
      <outputs>
        <output reference="mus1_right_strength1"/>
        <output reference="mus1_right_strength2"/>
      </outputs>
    </computedOutput>

    <computedOutput>
      <code>Average_Left_Hand_Muscle_Strength_Measurement</code>
      <measurementUnit>kg</measurementUnit>
      <dataType>INTEGER</dataType>
      <algorithm>AVERAGE</algorithm>
      <captureMethod>COMPUTED</captureMethod>
      <outputs>
        <output reference="mus1_left_strength1"/>
        <output reference="mus1_left_strength2"/>
      </outputs>
    </computedOutput>
  </instrumentParameters>
</instrumentType>
