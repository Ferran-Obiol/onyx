<stages>
  <stage>
    <name>StandingHeight</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variableName>consent</variableName>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>SittingHeight</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variableName>consent</variableName>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>HipsWaist</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variableName>consent</variableName>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>GripStrength</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variableName>consent</variableName>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>BloodPressure</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variableName>consent</variableName>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Impedence310</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variableName>consent</variableName>
        </variableCondition>
        <variableCondition>
          <stageName>StandingHeight</stageName>
          <variableName>height</variableName>
        </variableCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Weight</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variableName>consent</variableName>
        </variableCondition>
        <inverseCondition>
          <stageDependencyCondition class="variableCondition">
            <stageName>Impedence310</stageName>
            <variableName>weight</variableName>
          </stageDependencyCondition>
        </inverseCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Spirometry</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variableName>consent</variableName>
        </variableCondition>
        <variableCondition>
          <stageName>StandingHeight</stageName>
          <variableName>height</variableName>
        </variableCondition>
        <multipleCondition>
          <operator>OR</operator>
          <conditions>
            <variableCondition>
              <stageName>Impedence310</stageName>
              <variableName>weight</variableName>
            </variableCondition>
            <variableCondition>
              <stageName>Weight</stageName>
              <variableName>weight</variableName>
            </variableCondition>
          </conditions>
        </multipleCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>ECG</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variableName>consent</variableName>
        </variableCondition>
        <variableCondition>
          <stageName>StandingHeight</stageName>
          <variableName>height</variableName>
        </variableCondition>
        <multipleCondition>
          <operator>OR</operator>
          <conditions>
            <variableCondition>
              <stageName>Impedence310</stageName>
              <variableName>weight</variableName>
            </variableCondition>
            <variableCondition>
              <stageName>Weight</stageName>
              <variableName>weight</variableName>
            </variableCondition>
          </conditions>
        </multipleCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>BoneDensity</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variableName>consent</variableName>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>ArtStiffness</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variableName>consent</variableName>
        </variableCondition>
        <variableCondition>
          <stageName>BloodPressure</stageName>
          <variableName>Systolic_Blood_Pressure</variableName>
        </variableCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>
</stages>