<stages>
  <stage>
    <name>StandingHeight</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>SittingHeight</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>HipsWaist</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
    <!-- PL: this is not working since question HW_CI_BP is a boilerplate and does no produce a variable.
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>CIPreliminaryQuestionnaire</stageName>
          <variablePath>/CARTAGENE/CIPreliminaryQuestionnaire/HW_CI_BP</variablePath>
          <operator>ne</operator>
          <data>
            <type>BOOLEAN</type>
            <value class="boolean">true</value>
          </data>
        </variableCondition>
      </conditions>
    </stageDependencyCondition>
     -->
  </stage>

  <stage>
    <name>GripStrength</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>BloodPressure</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Impedence310</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>StandingHeight</stageName>
          <variablePath>/CARTAGENE/StandingHeight/Output/CALC_AVG_HEIGHT</variablePath>
        </variableCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Weight</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </variableCondition>
        <inverseCondition>
          <stageDependencyCondition class="variableCondition">
            <stageName>Impedence310</stageName>
            <variablePath>/CARTAGENE/Impedence310/Output/RES_WEIGHT</variablePath>
          </stageDependencyCondition>
        </inverseCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Spirometry</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>StandingHeight</stageName>
          <variablePath>/CARTAGENE/StandingHeight/Output/CALC_AVG_HEIGHT</variablePath>
        </variableCondition>
        <multipleCondition>
          <operator>OR</operator>
          <conditions>
            <variableCondition>
              <stageName>Impedence310</stageName>
              <variablePath>/CARTAGENE/Impedence310/Output/RES_WEIGHT</variablePath>
            </variableCondition>
            <variableCondition>
              <stageName>Weight</stageName>
              <variablePath>/CARTAGENE/Weight/Output/RES_WEIGHT</variablePath>
            </variableCondition>
          </conditions>
        </multipleCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>ECG</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>StandingHeight</stageName>
          <variablePath>/CARTAGENE/StandingHeight/Output/CALC_AVG_HEIGHT</variablePath>
        </variableCondition>
        <multipleCondition>
          <operator>OR</operator>
          <conditions>
            <variableCondition>
              <stageName>Impedence310</stageName>
              <variablePath>/CARTAGENE/Impedence310/Output/RES_WEIGHT</variablePath>
            </variableCondition>
            <variableCondition>
              <stageName>Weight</stageName>
              <variablePath>/CARTAGENE/Weight/Output/RES_WEIGHT</variablePath>
            </variableCondition>
          </conditions>
        </multipleCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>BoneDensity</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>ArtStiffness</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>BloodPressure</stageName>
          <variablePath>/CARTAGENE/BloodPressure/Output/CALC_AVG_SYSTOLIC_BP</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>BloodPressure</stageName>
          <variablePath>/CARTAGENE/BloodPressure/Output/CALC_AVG_DIASTOLIC_BP</variablePath>
        </variableCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>
</stages>