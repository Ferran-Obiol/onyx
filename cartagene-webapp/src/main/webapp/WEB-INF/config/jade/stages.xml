<stages>
  <stage>
    <name>StandingHeight</name>
    <module>jade</module>
    <stageDependencyCondition class="stageVariableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
      <!--operator>eq</operator>
      <data type="BOOLEAN">
        <value class="boolean">true</value>
      </data-->
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>SittingHeight</name>
    <module>jade</module>
    <stageDependencyCondition class="stageVariableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>HipsWaist</name>
    <module>jade</module>
    <stageDependencyCondition class="stageVariableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>GripStrength</name>
    <module>jade</module>
    <stageDependencyCondition class="stageVariableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>BloodPressure</name>
    <module>jade</module>
    <stageDependencyCondition class="stageVariableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Impedence310</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <stageVariableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </stageVariableCondition>
        <stageVariableCondition>
          <stageName>StandingHeight</stageName>
          <variablePath>/CARTAGENE/StandingHeight/Out/Height</variablePath>
        </stageVariableCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Weight</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <stageVariableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </stageVariableCondition>
        <inverseCondition>
          <stageDependencyCondition class="stageVariableCondition">
            <stageName>Impedence310</stageName>
            <variablePath>/CARTAGENE/Impedence310/Out/Weight</variablePath>
          </stageDependencyCondition>
        </inverseCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Spirometry</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <stageVariableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </stageVariableCondition>
        <stageVariableCondition>
          <stageName>StandingHeight</stageName>
          <variablePath>/CARTAGENE/StandingHeight/Out/Height</variablePath>
        </stageVariableCondition>
        <multipleCondition>
          <operator>OR</operator>
          <conditions>
            <stageVariableCondition>
              <stageName>Impedence310</stageName>
              <variablePath>/CARTAGENE/Impedence310/Out/Weight</variablePath>
            </stageVariableCondition>
            <stageVariableCondition>
              <stageName>Weight</stageName>
              <variablePath>/CARTAGENE/Weight/Out/Weight</variablePath>
            </stageVariableCondition>
          </conditions>
        </multipleCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>ECG</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <stageVariableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </stageVariableCondition>
        <stageVariableCondition>
          <stageName>StandingHeight</stageName>
          <variablePath>/CARTAGENE/StandingHeight/Out/Height</variablePath>
        </stageVariableCondition>
        <multipleCondition>
          <operator>OR</operator>
          <conditions>
            <stageVariableCondition>
              <stageName>Impedence310</stageName>
              <variablePath>/CARTAGENE/Impedence310/Out/Weight</variablePath>
            </stageVariableCondition>
            <stageVariableCondition>
              <stageName>Weight</stageName>
              <variablePath>/CARTAGENE/Weight/Out/Weight</variablePath>
            </stageVariableCondition>
          </conditions>
        </multipleCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>BoneDensity</name>
    <module>jade</module>
    <stageDependencyCondition class="stageVariableCondition">
      <stageName>CON</stageName>
      <variablePath>/CARTAGENE/Consent/accepted</variablePath>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>ArtStiffness</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <stageVariableCondition>
          <stageName>CON</stageName>
          <variablePath>/CARTAGENE/Consent/accepted</variablePath>
        </stageVariableCondition>
        <stageVariableCondition>
          <stageName>BloodPressure</stageName>
          <variablePath>/CARTAGENE/BloodPressure/Out/Systolic_Blood_Pressure</variablePath>
        </stageVariableCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>
</stages>