<configurableAnswerProvider>
  <defaultAnswer>
    <categoryName>NO_ANSWER</categoryName><!-- Set default answer to NO_ANSWER. -->
  </defaultAnswer>
  <groups>
    <group>
      <name>NO_ANSWER_GROUP</name><!-- Set answer to questions in this group to NO_ANSWER. -->
      <questions>
        <string>Q1</string>
        <string>Q4</string>
      </questions>
      <answer>
        <categoryName>NO_ANSWER</categoryName>
      </answer>
    </group>
  </groups>
  <answers>
    <entry>
      <string>Q2</string><!-- Set Q2 (BIRTHDATE) answer to 1980. -->
      <categoryAnswer>
        <categoryName>1</categoryName>
        <dataType>INTEGER</dataType>
        <integerValue>1980</integerValue>
      </categoryAnswer>
    </entry>
    <entry>
      <string>Q3</string><!-- Set Q3 (BIRTHMONTH) answer to 12. -->
      <categoryAnswer>
        <categoryName>1</categoryName>
        <dataType>INTEGER</dataType>
        <integerValue>12</integerValue>
      </categoryAnswer>
    </entry>
  </answers>
</configurableAnswerProvider>