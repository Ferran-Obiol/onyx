<configurableAnswerProvider>
  <defaultAnswer>
    <categoryName>NO_ANSWER</categoryName><!-- Set default answer to NO_ANSWER. -->
  </defaultAnswer>
  <groups>
    <group>
      <name>DONT_KNOW_GROUP</name><!-- Set answer to questions in this group to DONT_KNOW. -->
      <questions>
        <string>Q1</string>
        <string>Q4</string>
      </questions>
      <answer>
        <categoryName>DONT_KNOW</categoryName>
      </answer>
    </group>
  </groups>
  <answers>
    <entry>
      <string>Q2</string><!-- Set Q2 (BIRTHDATE) answer to 1980. -->
      <categoryAnswer reference="cat_1"/>
    </entry>
    <entry>
      <string>Q3</string><!-- Set Q3 (BIRTHMONTH) answer to 12. -->
      <categoryAnswer reference="cat_2"/>
    </entry>
  </answers>
</configurableAnswerProvider>

<categoryAnswer id="cat_1">
  <categoryName>1</categoryName>
  <openAnswer>
    <dataType>INTEGER</dataType>
    <integerValue>1980</integerValue>
  </openAnswer>
</categoryAnswer>

<categoryAnswer id="cat_2">
  <categoryName>1</categoryName>
  <openAnswer>
    <dataType>INTEGER</dataType>
          <integerValue>12</integerValue>
  </openAnswer>
</categoryAnswer> 