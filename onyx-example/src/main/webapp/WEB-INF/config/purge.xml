<list>
  <purge>
    <valueset entityType="Participant">
      <entities>
        <excludeAll type="EXCLUDE" />
        <script type="INCLUDE">
          <javascript>
            if($('Admin.Participant.exported').any('TRUE') == false) {
              false;
            } else {
              var purgeDate = $('Admin.Interview.endDate').add(onyx('org.obiba.onyx.participant.purge'));
              now().after(purgeDate);
              true;
            }
          </javascript>
        </script>
        <script type="INCLUDE">
          <javascript>
            if($('Admin.Interview.endDate').isNull()) {
              false;
            } else {
              $('Admin.Interview.status').any('CLOSED','CANCELED').and($('Admin.Interview.endDate').before(onyx('org.obiba.onyx.lastExportDate')))
            }
          </javascript>
        </script>
      </entities>
    </valueset>
  </purge>
</list>
