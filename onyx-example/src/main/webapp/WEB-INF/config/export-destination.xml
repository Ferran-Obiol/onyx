<destinations>
  <destination>
    <name>DCC</name>
    <strategies>
      <strategy>encrypt</strategy>
      <strategy>zip</strategy>
      <strategy>file</strategy>
    </strategies>
    
    <valueset entityType="Participant">
      <entities>
        <script type="include">
          // Include any ValueSet that has 'CLOSED' or 'COMPLETED' as a value for the 'Participant.Interview.Status' variable
          $('Participant.Interview.status').any('CLOSED','COMPLETED')
        </script>
        <script type="exclude">
          $('Admin.Interview.exported').any('TRUE')
        </script>
      </entities>
      <variables>
        <variableName type="exclude" prefix="Consent.pdfForm" />
        <variableName type="exclude" prefix="Admin.Participant" />
        <variableName type="include" prefix="Admin.Participant.barcode" />
        <variableName type="include" prefix="Admin.Participant.PostalCodePrefix" />
      </variables>
    </valueset>
      
    <valueset entityType="Workstation">
      <entities>
        <script type="exclude">
          $('Workstation.exported').any('TRUE')
        </script>
      </entities>
    </valueset>

    <valueset entityType="Instrument">
      <entities>
        <script type="exclude">
          $('Instrument.exported').any('TRUE')
        </script>
      </entities>
    </valueset>
  </destination>
      
    
    
  <destination>
    <name>Appointment Unit</name>
    <strategies>
      <strategy>zip</strategy>
      <strategy>file</strategy>
    </strategies>
    
    <valueset entityType="Participant">
      <entities>
        <script type="include">
          // Include any ValueSet that has 'CLOSED' or 'COMPLETED' as a value for the 'Participant.Interview.Status' variable
          $('Participant.Interview.status').any('CLOSED','COMPLETED')
        </script>
        <script type="exclude">
          $('Admin.Interview.exported').any('TRUE')
        </script>
      </entities>
      <variables>
        <excludeAll />        
        <variableName type="include" prefix="Consent" />
        <variableName type="include" prefix="Admin.onyxVersion" />
        <variableName type="exclude" prefix="Consent.actions" />
      </variables>
    </valueset>       
  </destination>
  
  <destination>
    <name>Appointment Unit - Cancelled interviews</name>
    <strategies>
      <strategy>zip</strategy>
      <strategy>file</strategy>
    </strategies>
    
    <valueset entityType="Participant">
      <entities>
        <script type="include">
          // Include any ValueSet that has 'CLOSED' or 'COMPLETED' as a value for the 'Participant.Interview.Status' variable
          $('Participant.Interview.status').any('CANCELLED')
        </script>
        <script type="exclude">
          $('Admin.Interview.exported').any('TRUE')
        </script>
      </entities>
      <variables>
        <excludeAll />        
        <variableName type="include" prefix="Consent" />
        <variableName type="include" prefix="Admin.onyxVersion" />
        <variableName type="exclude" prefix="Consent.actions" />
      </variables>
    </valueset>       
  </destination>
</destinations>