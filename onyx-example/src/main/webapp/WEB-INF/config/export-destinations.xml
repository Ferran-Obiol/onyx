<destinations>

  <destination>
    <name>DCC</name>

    <encrypt>
      <algorithm>DESede</algorithm>
      <mode>CBC</mode>
      <padding>PKCS5Padding</padding>
      <keySize>112</keySize>
    </encrypt>

    <valueset entityType="Participant">
      <entities>
        <excludeAll />
        <script type="INCLUDE">
          <javascript><![CDATA[// Include any ValueSet that has 'CLOSED' or 'COMPLETED' as a value for the 'Participant.Interview.Status' variable
          $('Admin.Interview.status').any('CLOSED','COMPLETED')]]></javascript>
        </script>
        <script type="EXCLUDE">
          <javascript><![CDATA[$('Admin.Interview.exportLog.destination').any('DCC')]]></javascript>
        </script>
      </entities>
      <variables>
        <variableName type="EXCLUDE" prefix="Consent.pdfForm" />
        <variableName type="EXCLUDE" prefix="Admin.Participant" />
        <variableName type="INCLUDE" prefix="Admin.Participant.barcode" />
        <variableName type="INCLUDE" prefix="Admin.Participant.PostalCodePrefix" />
      </variables>
    </valueset>

    <valueset entityType="Workstation">
      <entities>
        <script type="EXCLUDE">
          <javascript><![CDATA[$('Workstation.exportLog.destination').any('DCC')]]></javascript>
        </script>
      </entities>
    </valueset>

    <valueset entityType="Instrument">
      <entities>
        <script type="EXCLUDE">
          <javascript><![CDATA[$('Instrument.exportLog.destination').any('DCC')]]></javascript>
        </script>
      </entities>
    </valueset>
  </destination>

  <destination>
    <name>Appointment Unit</name>
    <encrypt />
    <valueset entityType="Participant">
      <entities>
        <excludeAll />
        <script type="INCLUDE">
          <javascript><![CDATA[// Include any ValueSet that has 'CLOSED' or 'COMPLETED' as a value for the 'Participant.Interview.Status' variable
          $('Admin.Interview.status').any('CLOSED','COMPLETED')]]></javascript>
        </script>
        <script type="EXCLUDE">
          <javascript><![CDATA[$('Admin.Interview.exportLog.destination').any('Appointment Unit')]]></javascript>
        </script>
      </entities>
      <variables>
        <excludeAll />
        <variableName type="INCLUDE" prefix="Consent" />
        <variableName type="INCLUDE" prefix="Admin.onyxVersion" />
        <variableName type="EXCLUDE" prefix="Consent.actions" />
      </variables>
    </valueset>
  </destination>

  <destination>
    <name>Appointment Unit - Cancelled interviews</name>

    <valueset entityType="Participant">
      <entities>
        <excludeAll />
        <script type="INCLUDE">
          <javascript><![CDATA[// Include any ValueSet that has 'CANCELLED' as a value for the 'Participant.Interview.Status' variable
          $('Admin.Interview.status').any('CANCELLED')]]></javascript>
        </script>
        <script type="EXCLUDE">
          <javascript><![CDATA[$('Admin.Interview.exportLog.destination').any('Appointment Unit - Cancelled interviews')]]></javascript>
        </script>
      </entities>
      <variables>
        <excludeAll />
        <variableName type="INCLUDE" prefix="Consent" />
        <variableName type="INCLUDE" prefix="Admin.onyxVersion" />
        <variableName type="EXCLUDE" prefix="Consent.actions" />
      </variables>
    </valueset>
  </destination>
    
</destinations>
