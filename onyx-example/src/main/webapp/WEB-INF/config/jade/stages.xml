  <stage>
    <name>Impedance310</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>Consent</stageName>
          <variablePath>Consent.accepted</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>CIPreliminaryQuestionnaire</stageName>
          <variablePath>CIPreliminaryQuestionnaire.QuestionnaireRun.timeEnd</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>CIPreliminaryQuestionnaire</stageName>
          <variablePath>CIPreliminaryQuestionnaire.STAGE_STATUS.BI_CI.active</variablePath>
          <operator>ne</operator>
          <data>
            <type>BOOLEAN</type>
            <value class="boolean">true</value>
          </data>
        </variableCondition>
        <variableCondition>
          <stageName>StandingHeight</stageName>
          <variablePath>StandingHeight.CALC_AVG_HEIGHT</variablePath>
        </variableCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Weight</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>Consent</stageName>
          <variablePath>Consent.accepted</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>CIPreliminaryQuestionnaire</stageName>
          <variablePath>CIPreliminaryQuestionnaire.QuestionnaireRun.timeEnd</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>CIPreliminaryQuestionnaire</stageName>
          <variablePath>CIPreliminaryQuestionnaire.STAGE_STATUS.WT_CI.active</variablePath>
          <operator>ne</operator>
          <data>
            <type>BOOLEAN</type>
            <value class="boolean">true</value>
          </data>
        </variableCondition>
        <inverseCondition>
          <stageDependencyCondition class="variableCondition">
            <stageName>Impedance310</stageName>
            <variablePath>Impedance310.CALC_AVG_WEIGHT</variablePath>
          </stageDependencyCondition>
        </inverseCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>Spirometry</name>
    <module>jade</module>
    <stageDependencyCondition class="multipleCondition">
      <operator>AND</operator>
      <conditions>
        <variableCondition>
          <stageName>Consent</stageName>
          <variablePath>Consent.accepted</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>CIPreliminaryQuestionnaire</stageName>
          <variablePath>CIPreliminaryQuestionnaire.QuestionnaireRun.timeEnd</variablePath>
        </variableCondition>
        <variableCondition>
          <stageName>CIPreliminaryQuestionnaire</stageName>
          <variablePath>CIPreliminaryQuestionnaire.STAGE_STATUS.SP_CI.active</variablePath>
          <operator>ne</operator>
          <data>
            <type>BOOLEAN</type>
            <value class="boolean">true</value>
          </data>
        </variableCondition>
        <variableCondition>
          <stageName>StandingHeight</stageName>
          <variablePath>StandingHeight.CALC_AVG_HEIGHT</variablePath>
        </variableCondition>
        <multipleCondition>
          <operator>OR</operator>
          <conditions>
            <variableCondition>
              <stageName>Impedance310</stageName>
              <variablePath>Impedance310.CALC_AVG_WEIGHT</variablePath>
            </variableCondition>
            <variableCondition>
              <stageName>Weight</stageName>
              <variablePath>Weight.RES_WEIGHT</variablePath>
            </variableCondition>
          </conditions>
        </multipleCondition>
      </conditions>
    </stageDependencyCondition>
  </stage>

  <stage>
    <name>ECG</name>
    <module>jade</module>
    <stageDependencyCondition class="variableCondition">
